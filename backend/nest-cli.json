{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "../prisma/schema.prisma",
        "outDir": "dist/prisma"
      },
      {
        "include": "modules/jobs/heat-rules.yaml",
        "outDir": "dist/modules/jobs"
      }
    ]
  }
}
