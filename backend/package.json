{
  "name": "job-hunt-backend",
  "version": "0.1.0",
  "description": "Job Hunt Management System backend (NestJS + Prisma)",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "bash -lc 'set -a && source ../.env && set +a && node dist/main.js'",
    "start:dev": "bash -lc 'set -a && source ../.env && set +a && npx nest start --env-file ../.env --watch'",
    "start:prod": "bash -lc 'set -a && source ../.env && set +a && node dist/main.js'",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate": "bash -lc 'set -a && source ../.env && set +a && npx prisma migrate dev'",
    "prisma:generate": "bash -lc 'set -a && source ../.env && set +a && npx prisma generate'",
    "prisma:seed": "bash -lc 'set -a && source ../.env && set +a && ts-node --transpile-only src/prisma/seed.ts'",
    "prisma:reset": "bash -lc 'set -a && source ../.env && set +a && npx prisma migrate reset --force --skip-generate --skip-seed && npm run prisma:seed'"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@prisma/client": "^5.9.0",
    "argon2": "^0.31.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "jose": "^5.2.0",
    "node-cron": "^3.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.10.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.9.0",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "../prisma/schema.prisma"
  },
  "license": "MIT"
}
