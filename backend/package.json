{
  "name": "job-hunt-backend",
  "version": "0.1.0",
  "description": "Job Hunt Management System backend (NestJS + Prisma)",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node ../scripts/use-env.js --env production node dist/main.js",
    "start:dev": "node ../scripts/use-env.js --env development nest start --watch",
    "start:prod": "node ../scripts/use-env.js --env production node dist/main.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --cache --cache-location .eslintcache --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --cache --cache-location .eslintcache --fix",
    "lint:fix:mem": "NODE_OPTIONS=--max-old-space-size=4096 eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "node ../scripts/use-env.js --env development jest --config ./test/jest-e2e.json",
    "prisma:migrate": "node ../scripts/use-env.js --env development prisma migrate dev",
    "prisma:generate": "node ../scripts/use-env.js --env development prisma generate",
    "prisma:seed": "node ../scripts/use-env.js --env development ts-node --transpile-only src/prisma/seed.ts",
    "prisma:reset": "node ../scripts/use-env.js --env development prisma migrate reset --force --skip-generate --skip-seed && npm run prisma:seed"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@prisma/client": "^6.19.0",
    "argon2": "^0.31.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "jose": "^5.2.0",
    "node-cron": "^3.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.10.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.22.0",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "schema": "../prisma/schema.prisma"
  },
  "license": "MIT"
}
